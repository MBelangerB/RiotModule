{
  "name": "bedyriot",
  "version": "1.0.0",
  "description": "Bedy - Module for call Riot service and obtains information",
  "main": "build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "compile:npx": "npx tsc",
    "compile:rim": "rimraf ./build && tsc",
    "start:dev": "npx nodemon",
    "start:prod": "npm run compile:npx && node build/index.js",
    "local:pack": "npm run compile:npx && npm pack",
    "local:packCopy": "npm run compile:npx && npm pack && copy.bat",
    "preview": "npx npm-packlist",
    "eslint": "npx eslint .",
    "eslintFix": "npx eslint . --fix",
    "test:js_build": "rimraf ./build && tsc -p tsconfig-test.json",
    "test:js_run": "mocha ./build/test/*.spec.js --timeout 10000",
    "test:ts_run": "mocha -r ts-node/register test/**/*.spec.ts",
    "test:ts_coverage": "nyc --nycrc-path=./.nycrc_base.json mocha --timeout=10000",
    "test:ts_json_coverage": "nyc --nycrc-path=./.nycrc_git.json mocha --timeout=10000"
  },
  "files": [
    "/build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MBelangerB/RiotModule.git"
  },
  "author": "MBelangerB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MBelangerB/RiotModule/issues"
  },
  "homepage": "https://github.com/MBelangerB/RiotModule#readme",
  "dependencies": {
    "axios": "^1.2.5",
    "node-cache": "^5.1.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "chai": "^4.3.7",
    "dotenv": "^16.1.3",
    "eslint": "^8.33.0",
    "fs-extra": "^11.1.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}